int grade;
            int x = 0;
            
            ArrayList grArrList = new ArrayList();

            Console.Write("What is your homework score?");
            grade = int.Parse(Console.ReadLine());

            if (grade < 0 && grade > 10) 
            {
                Console.Write("Please give a different grade (0-10");
            } else {
                x++;
                grArrList.Add(grade);
            }
